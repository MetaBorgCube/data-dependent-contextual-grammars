regex /* was "token" */ {
InfixSymbol5Exclude
  ::= "|"
    | "||"
    | "&&"
    | "&"
    | "<-"
}

regex /* was "token" */ {
InfixSymbol6
  ::= "&"
    | "&&"
}

regex /* was "token" */ {
InfixSymbol7
  ::= "||"
    | "or"
}

regex /* was "token" */ {
InfixSymbol8
  ::= ":="
}

regex /* was "keywords" */ {
Keywords
  ::= "_"
    | "and"
    | "as"
    | "assert"
    | "asr"
    | "begin"
    | "class"
    | "constraint"
    | "do"
    | "done"

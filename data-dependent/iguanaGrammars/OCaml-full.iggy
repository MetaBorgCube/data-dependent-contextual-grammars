regex /* was "token" */ {
Whitespace
  ::= [\ \t\n\r]+
}

// TODO: unicode literal \u0009-\u000D not working
@Layout /* was "layout" */
Layout
  ::= (Comment | Whitespace)* !>> [\ \t\n\r] !>> "(*"
